{% extends "base.html.twig" %}
{% block body %}

    <div class="wrapper">

        <!-- Main Header -->
        {% block header %}
            {% include ":dashboard/partials:header.html.twig" %}
        {% endblock %}
        <!-- Left side column. contains the logo and sidebar -->
        {% block sidebar %}
            {% include ":dashboard/partials:sidebar.html.twig" %}
        {% endblock %}
        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper">
            <!-- Content Header (Page header) -->
            <section class="content-header">
                {% block contentHeader %}
                {% endblock %}
            </section>

            <!-- Main content -->
            <section class="content">
                <div class="row">
                    {% block content %}
                        <!-- Your Page Content Here -->
                    {% endblock %}
                </div>
            </section>
            <!-- /.content -->
        </div>
        <!-- /.content-wrapper -->
        {% block footer %}
            {% include ":dashboard/partials:footer.html.twig" %}
        {% endblock %}
        <!-- Main Footer -->
    </div>

{% endblock %}
